{
  "name": "yumma-css-helper",
  "displayName": "yumma-css-helper",
  "description": "A useful extension with Yumma CSS integration.",
  "version": "0.0.1",
  "publisher": "yummacss",
  "main": "extension.js",
  "icon": "icon.png",
  "activationEvents": [
    "onCommand:extension.generateSnippets"
  ],
  "author": {
    "name": "Renildo Pereira"
  },
  "categories": [
    "Snippets"
  ],
  "contributes": {
    "commands": [
      {
        "command": "extension.generateSnippets",
        "title": "Import Yumma CSS classes"
      }
    ],
    "snippets": [
      {
        "language": "html",
        "path": "./snippets/snippets.json"
      }
    ]
  },
  "dependencies": {
    "axios": "^1.5.0",
    "fs": "^0.0.1-security",
    "path": "^0.12.7",
    "postcss": "^8.4.29",
    "postcss-selector-parser": "^6.0.13"
  },
  "engines": {
    "vscode": "^1.82.0"
  },
  "keywords": [
    "yumma-css",
    "snippets"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rrenildopereiraa/yumma-css-helper"
  }
}
